        var getCookie = function(name) { return document.cookie.match(new RegExp('(^| )' + name + '=([^;]+)')) }
        var cookieManual = getCookie('sbrf.region_manual');
        var regionCookie = getCookie('sbrf.region_id');
        var regionSet = getCookie('sbrf.region_set');
