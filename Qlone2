            be.widgets = be.widgets || {};
            be.widgets.StaticHelper = {
                start: function (widget) {
                    if (window.bd && window.bd.designMode !== 'true' && dataLayer) {
                        var staticHtmlNode = widget.body.querySelector('.static-helper .bp-g-include');
                        var url = widget.getPreference('pageUrl') || '';
                        if (!url) {
                            dataLayer.push({event: 'SITE_Widget_StaticHelper', action: 'sumbit_error_no_settings', label: url});
                        } else if (!staticHtmlNode.querySelector('*')) {
                            dataLayer.push({event: 'SITE_Widget_StaticHelper', action: 'submit_error_page', label: url});
                        }
                    }

                    var rootNode = widget.body.querySelector('[data-widget-start]');
                    if (rootNode) {
                        var match = rootNode.dataset.widgetStart.match(/^([^(]+)\((.+)\)$/);

                        var chainStarter = match[1].split('.');
                        if (chainStarter[0] === 'window') {
                            chainStarter.shift();
                        }

                        var startFunction = chainStarter.reduce(function (result, current) {
                            if (result[current]) {
                                return result[current];
                            }
                            return result;
                        }, window);

                        if (startFunction) {
                            startFunction(Object.assign({}, widget, {
                                // и передаем ноду, в которую рисовать будем
                                body: rootNode,
                            }));
                        }
                    }

                }
            };
