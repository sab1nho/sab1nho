            function isNewBrowser(ua) {
                if (ua.indexOf('MSIE') !== -1 || ua.indexOf('Trident') !== -1) {
                    return false;
                }
                var uaArr = ua.match(/(opera|chrome|safari|firefox(?=\/))\/?\s*(\d+)/i) || [];
                var browser = uaArr[1];
                var version = parseInt(uaArr[2], 10);

                if (uaArr[1] === 'Chrome') {
                    var others = ua.match(/\b(OPR|Edg)\/(\d+)/);
                    if (others !== null) {
                        browser = others[1].replace('OPR', 'Opera');
                        version = parseInt(others[2], 10);
                    }
                } else {
                    uaArr = uaArr[2] ? [uaArr[1], uaArr[2]] : [ navigator.appName, navigator.appVersion, '-?' ];
                    var v = ua.match(/version\/(\d+)/i);
                    if (v !== null) {
                        uaArr.splice(1, 1, uaArr[1]);
                    }
                    browser = uaArr[0];
                    version = parseInt(uaArr[1], 10);
                }

                return (['Chrome', 'Firefox', 'Safari', 'Opera', 'Edg'].indexOf(browser) === -1) ||
                    browser === 'Chrome' && version > 74 ||
                    browser === 'Firefox' && version > 74 ||
                    browser === 'Safari' && version > 601 ||
                    browser === 'Opera' && version > 74 ||
                    browser === 'Edg' && version > 89;
            }

            if (!isNewBrowser(navigator.userAgent)) {
                var script3 = document.createElement('script');
                script3.async = true;
                script3.src = polyfillScripts[2];
                document.head.appendChild(script3);
            }
